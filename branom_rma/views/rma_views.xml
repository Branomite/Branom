<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_rma_template_form_inherit" model="ir.ui.view">
        <field name="name">rma.template.form.inherit</field>
        <field name="model">rma.template</field>
        <field name="inherit_id" ref="rma.view_rma_template_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='usage']" position="after">
                <field name="branom_sequence_suffix"/>
            </xpath>
        </field>
    </record>

</odoo>

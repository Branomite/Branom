<odoo>
    <data>
        <record id="view_pricelist_form_inherit_branom" model="ir.ui.view">
            <field name="name">product.pricelist.form.inherit.branom</field>
            <field name="model">product.pricelist</field>
            <field name="inherit_id" ref="product.product_pricelist_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='country_group_ids']" position="after">
                    <field name="is_commission"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
